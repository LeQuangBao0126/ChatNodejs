<div class="modal fade" id="notificationModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">All Notification.</h4>
            </div>
            <div class="modal-body">
                <a href="javascript:void(0)" id="popup-mark-all-as-read">Đánh dấu tất cả là đã đọc</a>
                <ul class="list-notifications">
                    <%notifications.forEach(notification=>{%>
                        <li>
                            <%- notification %>
                        </li>
                        <%})%>
                </ul>
            </div>
            <div class="read-more-notif">
                <a href="javascript:void(0)" id="link-read-more-notif">Xem Thêm 10 Thông Báo</a>
            </div>
        </div>
    </div>
</div>
<script>markNotifAsRead()</script>